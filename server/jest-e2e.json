{
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "json"
  ],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "testRegex": "/src/.*\\.(test|e2e-spec).(ts|tsx|js)$",
  "collectCoverageFrom": [
    "src/**/*.{js,jsx,tsx,ts}",
    "!**/node_modules/**",
    "!**/vendor/**"
  ],
  "coverageDirectory": "./coverage-e2e",
  "coverageReporters": [
    "json",
    "lcov"
  ]
}